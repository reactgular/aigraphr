        "/projects/files": {
            "get": {
                "operationId": "ProjectsFilesController_index",
                "parameters": [
                    {
                        "name": "sort",
                        "required": false,
                        "in": "query",
                        "schema": {
                            "default": "asc",
                            "enum": [
                                "asc",
                                "desc"
                            ],
                            "type": "string"
                        }
                    },
                    {
                        "name": "sortBy",
                        "required": false,
                        "in": "query",
                        "schema": {
                            "default": "createdAt",
                            "enum": [
                                "createdAt",
                                "name"
                            ],
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
