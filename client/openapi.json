                        "example": "344feee1cda9f0d3b68d14cb0586e948c72bad81f96a8fba4311376b62545dde"
                    },
                    "name": {
                        "type": "string",
                        "description": "Name of the project",
                        "example": "My project"
                    },
                    "file": {
                        "description": "Project file",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/ProjectFileDto"
                            }
                        ]
                    }
                },
                "required": [
                    "id",
                    "fileId",
                    "name"
                ]
            },
            "ProjectInstanceDto": {
                "type": "object",
