    "components": {
        "securitySchemes": {
            "bearer": {
                "scheme": "bearer",
                "bearerFormat": "JWT",
                "type": "http"
            }
        },
        "schemas": {
            "ProjectFileDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "The unique identifier of the project file",
                        "example": "344feee1cda9f0d3b68d14cb0586e948c72bad81f96a8fba4311376b62545dde"
                    },
                    "name": {
                        "type": "string",
                        "description": "The name of the project (alphanumeric)",
                        "example": "example-project"
                    },
                    "fileName": {
                        "type": "string",
                        "description": "The name of the project file",
                        "example": "example-project.aigraphr"
                    },
                    "size": {
                        "type": "number",
                        "description": "The size of the project file in bytes",
                        "example": 1024
                    },
                    "folder": {
                        "type": "string",
                        "description": "The folder where the project file is stored",
                        "example": "/path/to"
                    },
                    "path": {
                        "type": "string",
                        "description": "The path to the project file",
                        "example": "/path/to/example-project.aigraphr"
                    },
                    "createdAt": {
                        "format": "date-time",
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "name",
                    "fileName",
                    "size",
                    "folder",
                    "path",
                    "createdAt"
                ]
            },
            "ProjectInstanceWithFileDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "ID of the project in memory",
                        "example": "344feee-1cda-9f0d-3b68-d14cb0586e94"
